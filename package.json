{
  "name": "my-app",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. .",
    "serve": "rm -fr tf-pixi-sample && cp -r docs tf-pixi-sample && npx http-server -o /tf-pixi-sample"
  },
  "dependencies": {
    "@pixi/filter-pixelate": "^4.1.3",
    "@tensorflow-models/body-segmentation": "^1.0.1",
    "@tensorflow-models/face-detection": "^1.0.1",
    "@tensorflow/tfjs-backend-webgl": "^3.18.0",
    "@tensorflow/tfjs-converter": "^3.18.0",
    "@tensorflow/tfjs-core": "^3.18.0",
    "pixi.js": "^6.4.2"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "sass": "^1.53.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vite-plugin-externals": "^0.5.1"
  }
}
